<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.aboutcoder.ssm.persistence.GuideBasicMapper">
    <resultMap id="BaseResultMap" type="com.aboutcoder.ssm.domain.po.GuideBasic">
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="job_type" property="jobType" jdbcType="INTEGER" />
    </resultMap>
    <sql id="Base_Column_List">
        id,job_type
    </sql>

    <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Integer">
        select
        <include refid="Base_Column_List" />
        from guide_basic
        where id = #{guideId,jdbcType=INTEGER}
    </select>
</mapper>
